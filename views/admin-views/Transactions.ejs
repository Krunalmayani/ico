<%- include('header') %>

    <div class="page-content">
        <div class="container">
            <div class="card content-area">
                <div class="card-innr">
                    <div class="card-head">
                        <h4 class="card-title">Transactions</h4>
                    </div>
                    <table class="data-table transpro dt-init user-tnx">
                        <thead>
                            <tr class="data-item data-head">
                                <th class="data-col dt-tnxno">Date</th>
                                <th class="data-col dt-tnxno">User</th>
                                <th class="data-col dt-tnxno">USD</th>
                                <th class="data-col dt-tnxno">Pay Amount</th>
                                <th class="data-col dt-tnxno">Coin</th>
                                <th class="data-col dt-tnxno">Bonus</th>
                                <th class="data-col dt-tnxno">Total Coins</th>
                                <th class="data-col dt-tnxno">Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <%for(var i of data){%>
                                <tr class="data-item">
                                    <td class="data-col dt-tnxno">
                                        <div class="d-flex align-items-center">
                                            <div class="fake-class">
                                                <span class="lead tnx-id">
                                                    <%= i.Transaction_submit_Time.toDateString() %>
                                                </span>

                                            </div>
                                        </div>
                                    </td>
                                    <td class="data-col dt-token">
                                        <span class="lead token-amount">
                                            <%= i.User_id.Fname %>
                                        </span>
                                        <span class="sub sub-symbol">
                                            <%= i.User_id.email %>
                                        </span>
                                    </td>

                                    <td class="data-col dt-usd-amount">
                                        <span class="lead amount-pay">
                                            $<%= i.Usd %>
                                        </span>

                                    </td>
                                    <td class="data-col dt-account">
                                        <span class="lead amount-pay">
                                            <% if (i.Method==="Card" ) { %>
                                                <span class="lead amount-pay">
                                                    Card Payment
                                                </span>
                                                <% } else { %>
                                                    <%= i.Pay_Amount %>

                                                        <% } %>
                                        </span>
                                        <span class="sub sub-symbol">
                                            <% if (i.Method==="ethereum" ) { %>
                                                <a style="text-transform: capitalize; font-weight: bold"
                                                    href="https://etherscan.io/address/<%= i.Crypto_Address %>"
                                                    target="_blank">
                                                    <%= i.Method %>
                                                        <em style="font-size: 10px; font-weight: 900;"
                                                            class="ti ti-search"></em>
                                                </a>
                                                <% } else if (i.Method==="bitcoin" ) { %>
                                                    <a style="text-transform: capitalize; font-weight: bold"
                                                        target="_blank"
                                                        href="https://bitaps.com/<%= i.Crypto_Address %>">
                                                        <%= i.Method %>
                                                            <em style="font-size: 10px; font-weight: 900;"
                                                                class="ti ti-search"></em>
                                                    </a>
                                                    <% } else if (i.Method==="solana" ) { %>

                                                        <a style="text-transform: capitalize; font-weight: bold;"
                                                            href="https://solscan.io/search?q=<%= i.Crypto_Address %>"
                                                            target="_blank">
                                                            <%= i.Method %>
                                                                <em style="font-size: 10px; font-weight: 900;"
                                                                    class="ti ti-search"></em>
                                                        </a>
                                                        <% } else if (i.Method==="USDT(ERC20)" ) { %>
                                                            <a style="font-weight: bold" target="_blank"
                                                                href="https://etherscan.io/token/0xdac17f958d2ee523a2206206994597c13d831ec7?a=<%= i.Crypto_Address %>">
                                                                <%= i.Method %>
                                                                    <em style="font-size: 10px; font-weight: 900;"
                                                                        class="ti ti-search"></em>
                                                            </a>
                                                            <% } else if (i.Method==="USDT(BEP20)" ) { %>

                                                                <a style="font-weight: bold" target="_blank"
                                                                    href="https://bscscan.com/token/0x55d398326f99059ff775485246999027b3197955?a=<%= i.Crypto_Address %>">
                                                                    <%= i.Method %>
                                                                        <em style="font-size: 10px; font-weight: 900;"
                                                                            class="ti ti-search"></em>
                                                                </a>
                                                                <% } else if (i.Method==="USDT(TRC20)" ) { %>
                                                                    <a style="font-weight: bold" target="_blank"
                                                                        href="https://tronscan.org/#/address/<%= i.Crypto_Address %>">
                                                                        <%= i.Method %>
                                                                            <em style="font-size: 10px; font-weight: 900;"
                                                                                class="ti ti-search"></em>
                                                                    </a>
                                                                    <% } else if (i.Method==="USDC(ERC20)" ) { %>

                                                                        <a style="font-weight: bold"
                                                                            href="https://etherscan.io/token/0xa0b86991c6218b36c1d19d4a2e9eb0ce3606eb48?a=<%= i.Crypto_Address %>">
                                                                            <%= i.Method %>
                                                                                <em style="font-size: 10px; font-weight: 900;"
                                                                                    class="ti ti-search"></em>
                                                                        </a>
                                                                        <% } else if (i.Method==="Card" ) { %>
                                                                            <h4 style="font-weight: bold; font-size: 12px;">
                                                                                <%= i.Pay_Transaction_id%>
                                                                            </h3>

                                                                            <% } else { %>

                                                                                <% } %>
                                        </span>
                                    </td>
                                    <td class="data-col dt-token">
                                        <span class="lead token-amount">
                                            <%= i.Token %>
                                        </span>
                                        <span class="sub sub-symbol">
                                            <%= webSettingData.Tokensymbol %>
                                        </span>
                                    </td>
                                    <td class="data-col dt-token">
                                        <span class="lead token-amount">
                                            <%= i.Bonus %>
                                        </span>
                                        <span class="sub sub-symbol">
                                            <%= webSettingData.Tokensymbol %>
                                        </span>
                                    </td>
                                    <td class="data-col dt-token">
                                        <span class="lead token-amount">
                                            <%= i.Total_Token %>
                                        </span>
                                        <span class="sub sub-symbol">
                                            <%= webSettingData.Tokensymbol %>
                                        </span>
                                    </td>
                                    <td class="data-col dt-type">
                                        <% if (i.Status==='Pending' ) { %>
                                            <span class="dt-status-md badge btn btn-warning badge-md">Pending</span>
                                            <span class="dt-status-sm badge badge-sq btn btn-warning badge-md">P</span>
                                            <% } else if (i.Status==='Approved' ) { %>
                                                <span
                                                    class="dt-status-md badge btn btn-success badge-md">Approved</span>
                                                <span class="dt-status-sm badge-sq btn btn-success badge-md">A</span>
                                                <% } else if (i.Status==='Rejected' ) { %>
                                                    <span
                                                        class="dt-status-md badge btn btn-danger badge-md">Rejected</span>
                                                    <span
                                                        class="dt-status-sm badge badge-sq btn btn-danger badge-md">R</span>
                                                    <% } %>
                                    </td>
                                    <td class="data-col text-right">
                                        <div class="relative d-inline-block">
                                            <a href="#" class="btn btn-light-alt btn-xs btn-icon toggle-tigger"><em
                                                    class="ti ti-more-alt"></em></a>
                                            <div class="toggle-class dropdown-content dropdown-content-top-left">
                                                <ul class="dropdown-list">
                                                    <li><a href="/admin/Transactions_approved/<%= i._id %>"><em
                                                                class="ti ti-check"></em> Approved</a></li>
                                                    <li><a href="/admin/Transactions_reject/<%=i._id%>"><em
                                                                class="ti ti-na"></em> Reject</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                                <%}%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <%- include('footer') %>